            <endImageSize value="6px 0" />
            <endImageColor value="$preset_fs25_colorGrey" />
            <endImageFocusedColor value="$preset_fs25_colorMainHighlight" />
            <endImageHighlightedColor value="$preset_fs25_colorMainHighlight"/>
            <endImageDisabledColor value="$preset_fs25_colorMainDark" />
            <endImageSliceId value="gui.3dShop_button_outline_right" />
        </Profile>
        <Profile name="rvb_button" extends="emptyPanel" with="anchorMiddleRight">
            <size value="316px 36px"/>
            <margin value="43px 24px 43px 24px"/>
            <iconSize value="22px 22px"/>
            <iconTextOffset value="0px -1px"/>
            <iconSliceId value="gui.tick"/>
            <iconColor value="$preset_fs25_colorMainLight_80"/>
            <iconFocusedColor value="$preset_fs25_colorMainHighlight"/>
            <iconHighlightedColor value="$preset_fs25_colorMainHighlight"/>
            <iconPressedColor value="$preset_fs25_colorMainLight_80"/>
            <iconDisabledColor value="$preset_fs25_colorMainLight_80"/>
            <textSize value="16px"/>
            <textBold value="true"/>
            <textUpperCase value="true"/>
            <textAlignment value="center"/>
            <textColor value="$preset_fs25_colorMainLight_80"/>
            <textFocusedColor value="$preset_fs25_colorMainHighlight"/>
            <textHighlightedColor value="$preset_fs25_colorMainHighlight"/>
            <textDisabledColor value="$preset_fs25_colorMainLight_80"/>
            <textPressedColor value="$preset_fs25_colorMainLight_80"/>
            <textFocusedColor value="$preset_fs25_colorMainDark"/>
        </Profile>
