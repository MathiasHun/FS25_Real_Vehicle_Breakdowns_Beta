    <GUIProfiles>
        <Profile name="rvb_boxLayout" extends="emptyPanel" with="anchorTopLeft">
            <position value="0px -31px"/>
            <size value="1550px 710px"/>
            <fitFlowToElements value="true"/>
            <flowDirection value="vertical" />
            <numFlows value="3"/>
            <wrapAround value="true"/>
        </Profile>
        <Profile name="rvb_multiTextOptionLeft" extends="buttonBase" with="anchorStretchingYLeft">
            <size value="40px 100%" />
            <clickSound value="NONE" />
            <sendActionOnRelease value="false" />
            <iconRotation value="180" />
            <iconSize value="7px 8px" />
            <iconTextOffset value="-7px 0px" />
            <iconSliceId value="gui.3dShop_arrow" />
            <iconColor value="$preset_fs25_colorMainLight_80" />
            <iconHighlightedColor value="$preset_fs25_colorMainHighlight" />
            <iconFocusedColor value="$preset_fs25_colorMainHighlight" />
            <iconPressedColor value="$preset_fs25_colorMainLight" />
            <iconDisabledColor value="$preset_fs25_colorMainLight_80" />
            <imageSliceId value="noSlice" />
            <imageColor value="0 0 0 0" />
            <imageHighlightedColor value="0 0 0 0" />
            <imageFocusedColor value="0 0 0 0" />
            <imageSelectedColor value="0 0 0 0" />
            <imageDisabledColor value="0 0 0 0" />
        </Profile>
        <Profile name="rvb_multiTextOptionRight" extends="rvb_multiTextOptionLeft" with="anchorStretchingYRight">
            <iconRotation value="0" />
            <iconTextOffset value="7px 0px" />
        </Profile>
        <Profile name="rvb_title" extends="baseReference" with="anchorMiddleLeft">
            <size value="210px 18px"/>
            <position value="12px 31px"/>
            <textSize value="18px"/>
            <textAlignment value="left"/>
            <textMaxWidth value="295px"/>
            <textMinSize value="10px"/>
            <textMaxNumLines value="1"/>
            <textAutoWidth value="true"/>
            <textLayoutMode value="resize"/>
            <textAutoWidth value="true"/>
            <textColor value="$preset_fs25_colorMainLight"/>
        </Profile>
        <Profile name="rvb_multiTextOption" extends="baseReference">
            <size value="316px 36px"/>
            <margin value="43px 20px 43px 20px"/>
            <hideButtonOnLimitReached value="false"/>
            <wrap value="true"/>
            <autoAddDefaultElements value="true"/>
            <defaultProfileButtonLeft value="rvb_multiTextOptionLeft"/>
            <defaultProfileButtonRight value="rvb_multiTextOptionRight"/>
            <defaultProfileText value="rvb_multiTextOptionText"/>
        </Profile>
        <Profile name="rvb_multiTextOption_First_line" extends="rvb_multiTextOption">
            <margin value="43px 30px 43px 20px"/>
        </Profile>
        <Profile name="rvb_multiTextOption_sec_column" extends="rvb_multiTextOption">
            <margin value="43px 116px 43px 20px"/>
        </Profile>
        <Profile name="rvb_multiTextOptionText" extends="rvb_textDefault" with="anchorStretchingYStretchingX">
            <size value="100% 100%"/>
            <absoluteSizeOffset value="50px 0px"/>
            <textSize value="16px"/>
            <textMaxNumLines value="1"/>
            <textUpperCase value="true"/>
            <textAlignment value="center"/>
            <textLayoutMode value="scrolling"/>
            <textScrollOnFocusOnly value="true"/>
            <textColor value="$preset_fs25_colorMainLight"/>
            <textDisabledColor value="$preset_fs25_colorMainLight_80"/>
        </Profile>
        <Profile name="rvb_textDefault" extends="baseReference">
            <size value="250px 20px"/>
            <textSize value="15px"/>
            <textAlignment value="left"/>
            <textColor value="$preset_fs25_colorMainLight"/>
            <textDisabledColor value="$preset_colorDisabled"/>
        </Profile>
        <Profile name="rvb_multiTextOptionBg" extends="rvb_binaryOptionBg"> <!-- fs25_multiTextOptionBg -->
            <imageDisabledColor value="$preset_fs25_colorMainDark"/>
