            <startImageColor value="$preset_fs25_colorGrey" />
            <startImageFocusedColor value="$preset_fs25_colorMainHighlight" />
            <startImageHighlightedColor value="$preset_fs25_colorMainHighlight"/>
            <startImageDisabledColor value="$preset_fs25_colorMainDark" />
